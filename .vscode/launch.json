    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Windows) Launch debug",
            "type": "cppvsdbg", // This is VS Code Extension
            "request": "launch",
            "program": "${workspaceRoot}/target/debug/groupware-warp.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "console": "externalTerminal",
            "preLaunchTask": "cargo debug",
            "sourceFileMap": {
                "/rustc/5e1a799842ba6ed4a57e91f7ab9435947482f7d8": "${env:USERPROFILE}/.rustup/toolchains/stable-x86_64-pc-windows-msvc/lib/rustlib/src/rust"
            },
            "symbolSearchPath": "https://msdl.microsoft.com/download/symbols"
        },
        {
            "name": "(Windows) Launch release",
            "type": "cppvsdbg", // This is VS Code Extension
            "request": "launch",
            "program": "${workspaceRoot}/target/release/groupware-warp.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "console": "externalTerminal",
            "preLaunchTask": "cargo release",
            "sourceFileMap": {
                "/rustc/5e1a799842ba6ed4a57e91f7ab9435947482f7d8": "${env:USERPROFILE}/.rustup/toolchains/stable-x86_64-pc-windows-msvc/lib/rustlib/src/rust"
            },
            "symbolSearchPath": "https://msdl.microsoft.com/download/symbols"
        },
        {
            "name": "(Windows) Launch seeder",
            "type": "cppvsdbg", // This is VS Code Extension
            "request": "launch",
            "program": "${workspaceRoot}/target/debug/groupware-warp.exe",
            "args": [
                "--seed"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "console": "externalTerminal",
            "preLaunchTask": "cargo debug",
            "sourceFileMap": {
                "/rustc/5e1a799842ba6ed4a57e91f7ab9435947482f7d8": "${env:USERPROFILE}/.rustup/toolchains/stable-x86_64-pc-windows-msvc/lib/rustlib/src/rust"
            },
            "symbolSearchPath": "https://msdl.microsoft.com/download/symbols"
        },
        {
            "name": "{OSX} Launch",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceRoot}/target/debug/groupware-warp",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=yourprogram"
                ],
                "filter": {
                    "name": "yourprogram",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "cargo build",
            "sourceLanguages": ["rust"],
            "sourceMap": {
                "/rustc/*": "${env:HOME}/.rustup/toolchains/stable-x86_64-apple-darwin/lib/rustlib/src/rust"
            }
        }
    ]
}